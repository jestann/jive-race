{
  "name": "jive-race",
  "author": "jestann",
  "description": "Team-building race app built on React",
  "engines": {
    "node": "8.7.0",
    "npm": "5.4.2"
  },
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jestann/jive-race.git"
  },
  "dependencies": {
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-markdown": "^2.5.0",
    "react-native-fs": "^2.8.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.13"
  },
  "scripts": {
    "watch-css": "npm run build-css && node-sass-chokidar src/styles -o src/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-css": "node-sass-chokidar src/styles -o src/css",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
