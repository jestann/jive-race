{
  "name": "jive-race",
  "author": "jestann",
  "description": "Team-building race app on React",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jestann/jive-race.git"
  },
  "engines": {
    "node": "8.7.0",
    "npm": "5.4.2"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-beta.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "jquery": "^3.2.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "popper.js": "^1.12.9",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-markdown": "^2.5.0",
    "react-native-fs": "^2.8.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.13",
    "react-test-renderer": "^16.2.0",
    "reactstrap": "^5.0.0-alpha.4",
    "sass": "^1.0.0-beta.4"
  },
  "scripts": {
    "watch-css": "npm run build-css && node-sass-chokidar src/styles -o src/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-css": "node-sass-chokidar src/styles -o src/css",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "jest": "^22.1.4",
    "jest-enzyme": "^4.1.1"
  }
}
